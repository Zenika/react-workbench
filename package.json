{
  "name": "zenika-react-workbench",
  "version": "0.0.1",
  "main": "src/script/index.js",
  "repository": "git@github.com:Zenika/react-workbench.git",
  "license": "MIT",
  "scripts": {
    "lint:js": "eslint --ext js,jsx src",
    "lint": "npm-run-all --parallel lint:*",
    "start": "./src/script/index.js ./misc/component.jsx",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "cross-env NODE_ENV=build rollup -c ./misc/rollup.config.js"
  },
  "bin": {
    "react-workbench": "./src/script/index.js"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-docgen": "^2.14.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "yargs": "^8.0.1"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2017",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./misc/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "./misc/test/testSetup.js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "babel"
    ],
    "globals": {
      "fetch": false
    },
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "arrow-body-style": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "func-names": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/no-danger": 0,
      "no-await-in-loop": 0
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
